.join.box h4, .membership .box h4 {display:block;padding:2vw 0;font-size:3.3vw;font-weight:500;color:#66aa0e}
.join.box , .membership .box {padding:1vw 2.5vw 3.5vw 4vw;}
.join.box p, 
.join.box li , 
.membership .box p, 
.membership .box li {font-size:3vw;line-height:5vw}