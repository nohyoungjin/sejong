@charset "utf-8";

/* 공통 */

h2.head_tit {padding:5vw 0 2vw;border-bottom:1px solid #d9d9d9;font-size:4.2vw;font-weight:700}
.sec_con p {font-size:3vw}

/* */

.tbl_type {margin:3vw 0}
.tbl_type table {width:100%;border:0;border-top:1px solid #a4a4a4}
.tbl_type caption {font-weight:bold;text-align:left}
.tbl_type th,
.tbl_type td {padding:2.4vw 1vw;border:0;font-size:3vw}

.tbl_type thead th {border:1px solid #a4a4a4;border-width:0 1px 1px 0;background:#f3f3f3;font-weight:500;vertical-align:middle;line-height:4vw}
.tbl_type tbody th {border:1px solid #a4a4a4;border-width:0 0 1px 0;background:#f3f3f3;color:#424242}
.tbl_type tbody td {border:1px solid #a4a4a4;border-width:0 1px 1px 0;text-align:center}

.tbl_type thead th:last-child {border-width:0 0 1px 0}
.tbl_type tbody td:last-child {border-width:0 0 1px 0}

/* 세종형 로컬푸드 */

.cont_local .intro .tit {display:block;padding:5vw 0 0;font-size:3.3vw;font-weight:500;color:#66aa0e;text-align:center}
.cont_local .intro .desc {padding:4vw 2vw}

.cont_local .area_cont {margin:5vw 0;border-top:1px dotted #959595}
.cont_local .area_cont strong {display:block;margin:4vw 0 0;font-size:3.4vw;color:#66aa0e}
.cont_local .area_cont img#img01 {display:block;width:68vw;height:74vw;margin:8vw auto 4vw}

.cont_local .area_cont p {padding:2vw 2vw 0 0}
.cont_local .area_cont p.note {position:relative;padding:2vw 0 4vw 1vw;color:#66aa0e}
.cont_local .area_cont p.note:before {content:"";display:inline-block;position:relative;top:50%;left:0;width:3.12vw;height:2.08vw;padding:0 0 0 2vw;background:url(../img/sejong/cont_arrow.png) no-repeat 0 0;background-size:3vw 2vw}

/* 왜 로컬푸드인가? */

.cont_why .intro .tit {display:block;padding:5vw 0 0;font-size:4vw;font-weight:500;color:#66aa0e;text-align:center}
.cont_why .intro .desc {padding:4vw 0;text-align:center}

.cont_why hr {display:block;width:4.6vw;height:0.3vw;margin:3vw auto 0;border:0;background-color:#66aa0e}
.cont_why .intro img#img01 {display:block;width:92vw;height:25vw;margin:4vw auto 8vw}

.cont_why .area_cont .tit {display:block;padding:2vw 0 0;font-size:4vw;font-weight:500;text-align:center}
.cont_why .area_cont .desc {padding:4vw 0 2vw;text-align:center}

.cont_why .area_cont img#img02 {display:block;width:85vw;height:46vw;margin:4vw auto 8vw}
.cont_why .area_cont img#img03 {display:block;width:92vw;height:44vw;margin:4vw auto 8vw}
.cont_why .area_cont img#img04 {display:block;width:48vw;height:47vw;margin:4vw auto 8vw}
.cont_why .area_cont img#img05 {display:block;width:92vw;height:31vw;margin:4vw auto 8vw}
.cont_why .area_cont img#img06 {display:block;width:92vw;height:55vw;margin:4vw auto 8vw}
.cont_why .area_cont img#img07 {display:block;width:92vw;height:66vw;margin:4vw auto 8vw}
.cont_why .area_cont img#img08 {display:block;width:92vw;height:66vw;margin:4vw auto 8vw}

/* 대표이사 인사말 */

.cont_ceo .intro p.txt {padding:6vw 0 0;font-size:3.4vw;color:#424242;text-align:center}
.cont_ceo .intro strong.tit {display:block;font-size:4vw;font-weight:500;color:#66aa0e;text-align:center}
.cont_ceo .intro img#img01 {display:block;width:72vw;height:auto;margin:6vw auto 4vw}

/* 싱싱장터에 가면 */

.cont_go .intro .tit {display:block;padding:5vw 0 3vw;font-size:4vw;font-weight:500;color:#66aa0e;text-align:center}
.cont_go .intro .desc {padding:0 0 3vw;color:#424242}

.cont_go .area_cont ul:after {display:block;clear:both;content:""}
.cont_go .area_cont ul li {float:left;width:33.3%;margin:2vw 0 0;text-align:center}
.cont_go .area_cont ul li img {width:22vw;height:22vw}

.cont_go .area_cont strong {display:block;position:relative;margin:4vw 0 0;padding:0 0 1vw 2vw;font-size:3.6vw;color:#424242}
.cont_go .area_cont strong:before {content:'';display:block;position:absolute;top:2.6vw;left:0;width:0.6vw;height:0.6vw;background-color:#81b952}

.cont_go .area_cont h3 {padding-top:1.8vw;font-size:3.3vw;font-weight:700;color:#66aa0e}
.cont_go .area_cont img#img1 {width:100%}

/* */

.cont_go .tbl_type {margin:5vw 0 6vw}
.cont_go .tbl_type tbody td:last-child {color:#66aa0e}

/* 싱싱장터 소개 */

.cont_info .intro {padding:3vw 0}
.cont_info .intro .desc {padding:0 0 1vw;color:#424242}
.cont_info .intro em {font-size:3vw;color:#d0151b}

.cont_info .area_cont strong {display:block;position:relative;margin:4vw 0 0;padding:0 0 1vw 2vw;font-size:3.6vw;color:#424242}
.cont_info .area_cont strong:before {content:'';display:block;position:absolute;top:2.6vw;left:0;width:0.6vw;height:0.6vw;background-color:#81b952}

.cont_info .area_cont img.organ {display:block;width:92vw;height:42vw;margin:6vw auto}

.cont_info .ver10 tbody th.t01 {border-right:1px solid #a4a4a4}
.cont_info .ver10 tbody th.t02 {padding:2vw 1.4vw}

.cont_info .ver10 tbody td {border:1px solid #a4a4a4;border-width:0 0 1px 1px}

.cont_info .ver11 tbody td:first-child {text-align:center}
.cont_info .ver11 tbody td {color:#424242;text-align:left}

.cont_info .ver11 ul li {position:relative;float:none;width:100%;margin:0.8vw 0 0;padding:0 0 0 4vw;font-size:3vw;text-align:left;line-height:4vw}
.cont_info .ver11 ul li:before {content:'';display:block;position:absolute;top:2vw;left:2.4vw;width:0.4vw;height:0.4vw;margin-top:-0.3vw;background-color:#81b952;border-radius:50%}

/* 지점안내 */

.cont_map .intro {padding:3vw 0}
.cont_map .intro .desc {padding:0 0 1vw;color:#424242}

.cont_map .tbl_type tbody th {border-width:0 1px 1px 0}
.cont_map .tbl_type tbody td {text-align:left}

.cont_map .tbl_type tbody tr:first-child th {border-width:1px 1px 1px 0}
.cont_map .tbl_type tbody tr:first-child td {border-width:1px 0 1px 0}

.cont_map .spot img {display:block}
.cont_map .spot .s01 {width:92vw;height:144vw;margin:5vw auto 7vw}
.cont_map .spot .s02 {width:68vw;height:128vw;margin:5vw auto 7vw}

.cont_map .area_cont.spot ul li {padding:2vw 0 5vw;text-align:center}
.cont_map .area_cont.spot ul li img {width:92vw;height:57vw;margin:0 auto 2vw}

/* 싱싱문화관은? */

.cont_culture .intro img#img01 {display:block;width:100%;height:51vw;margin:5vw auto 2vw}
.cont_culture intro p b {color:#66aa0e}

/* 층별안내도 */

.cont_floor .wrap_cont {padding:4vw 0}
.cont_floor img {width:100%;padding:3vw 0}

/* 내부시설안내 */

.cont_fixture .list {display:none} 
.cont_fixture .list.on {display:block !important}

.cont_fixture .list ul {padding:4vw 3vw}
.cont_fixture .list ul li {border:none !important;text-align:center}
.cont_fixture .list ul li img {width:100%}
.cont_fixture .list ul li span {display:block;max-width:100%;height:auto;margin:0 0 4vw}

/* 요리교실 */

.cont_cook .intro .tit {display:block;padding:5vw 0 3vw;font-size:4vw;font-weight:500;color:#66aa0e;text-align:center}
.cont_cook .intro .desc {padding:0 0 3vw;color:#424242}

.cont_cook .area_cont strong {display:block;position:relative;margin:5vw 0 1vw;color:#424242}
.cont_cook .area_cont p.desc {padding-left:0}
.cont_cook .area_cont p.desc strong {margin:0 0 2vw;font-size:3vw;font-weight:500}

.cook .area_cont ul li {margin:2vw 0;border:1px solid #cecece}
.cook .area_cont ul li h2 {padding:2.6vw;background:#eee;color:#424242;font-size:3.5vw;font-weight:500}
.cook .area_cont ul.tx01 {margin-top:0;padding:2vw 3vw 3vw} 
.cook .area_cont ul.tx01 li:before {content:"*";position:relative;top:0.5vw;padding-right:1vw}

.cook .area_cont ul.tx01 li, 
.cook .area_cont ul.tx02 li {clear:both;margin:0;padding:0.3vw 0;border:none;font-size:2.9vw}

.cook .area_cont ul.tx02 {padding:0 0 4vw 5vw}
.cook .area_cont ul.tx02 li:after {display:block;clear:both;content:""}
.cook .area_cont ul.tx02 p {float:left;padding:0;font-size:2.9vw}

.cook .area_cont h3 {position:relative;float:left;width:22vw;font-size:3vw;font-weight:500;color:#66aa0e}

.cont_cook .area_cont em.em {display:inline-block;margin:2vw 0 0 0.1vw;font-size:3.8vw;font-weight:700;color:#66aa0e}
.cont_cook ul.re {border-bottom:1px dotted #a4a4a4}
.cont_cook ul.re {margin:1vw 0 0;padding:0 0 3vw}
.cont_cook ul.re li {position:relative;margin:0;padding:0 0 1vw 3vw;border:0;font-weight:700;line-height:5vw !important;letter-spacing:-0.5px}
.cont_cook ul.re li:before {content:'';display:block;position:absolute;top:2.2vw;left:0;width:1.4vw;height:0.3vw;background-color:#333}

/* list */

.cont_cook .area_cont span.m {display:inline-block}
.cont_cook .area_cont strong.s1 {display:block;margin:3vw 0 0 3vw;font-weight:500;text-align:left}

/* view */

.cook .payment {margin:3vw 0 0;padding:4vw 0;border-top:1px dotted #a4a4a4;border-bottom:1px dotted #a4a4a4}

.cook .metro:after {display:block;clear:both;content:""}
.cook .metro li {float:left;width:48%;height:27vw;margin:2% 2% 2% 0;padding:6vw 0;border:1px solid #b5b5b5;text-align:center}
.cook .metro li a {display:block;font-size:3.1vw}

.cook .metro li img {display:block;width:14.37vw;height:10.13vw;margin:0 auto 2.5vw}
.cook .metro li + li img {width:11.66vw;height:9.37vw}

.metro li.selected {background:#91c069;color:#fff}

.cook p.txt01 {margin:4vw 0 0;font-size:2.7vw;color:#66aa0e;text-align:center}

.cont_cookView .item {padding:5vw 0;border-bottom:1px dotted #a4a4a4;text-align:center}
.cont_cookView .item label {padding:0 7vw 0 3vw}
.cont_cookView .item .label.t_radio:before {position:absolute;left:auto;right:0}
.cont_cookView .item section {width:auto;margin:0 auto}

/* complete */

.cont_cookComplete .intro {margin:3vw 0 4vw;font-size:3vw}
.cont_cookComplete .intro em {display:block;margin:0 0 1vw;font-size:3.5vw;font-weight:500;color:#000}

/* 싱싱밥상 */

.cont_eat .intro {margin:3vw 0 4vw;font-size:3vw}

.cont_eat .area_cont {padding:1vw 0}
.cont_eat .area_cont ul li {width:100%;margin:2vw 0 5vw;text-align:center}
.cont_eat .area_cont ul li img {width:96vw;height:57.15vw;margin:0 auto 2vw}
.cont_eat .area_cont ul li span {display:block;width:95vw;margin-bottom:5vw}

/* 커뮤니티 리스트 */

.lst_noti {width:100%;padding:3vw 0 0}
.lst_noti li {position:relative;border-top:1px solid #d9d9d9}

.lst_noti li:before {content:"";display:block;position:absolute;top:50%;right:2.33vw;width:3.6vw;height:8vw;margin-top:-1vw;background:url(../img/board/bu_arrow_down02.png) no-repeat 0 0;background-size:60%}
.lst_noti li.on a:before {background-image:url(../img/board/bu_arrow_down02_on.png)}

.lst_noti li:first-child {border-top:none}

.lst_noti li a {display:block;position:relative;text-decoration:none}
.lst_noti li a.download {overflow:hidden;float:left;max-width:50vw;margin-right:1vw;white-space:nowrap;text-overflow:ellipsis}

.lst_noti li a section {padding:2vw 10vw 8vw 2vw}

.lst_noti li .noti {overflow:hidden;float:left;max-width:70%;margin-right:15px;color:#66aa0e;text-overflow:ellipsis}
.lst_noti li .date {display:block;float:left;font-size:2.7vw;color:#a3a3a3}

.lst_noti li .tit {clear:both;display:block;margin:1.53vw 0}
.lst_noti li.on .tit {color:#ed145b}

.lst_noti li .tit p {overflow:hidden;float:left;max-width:70%;margin-right:1vw;white-space:nowrap;text-overflow:ellipsis}
.lst_noti li .tit img {float:left;width:3.6vw;margin-left:1vw}

/* 커뮤니티 뷰 */

.noti_view {padding:4vw 0}
.noti_view section {padding:0 3vw 5vw 2vw}

.noti_view .tit {display:block;overflow:visible;max-width:100%;white-space:normal}
.noti_view .tit img {position:relative;top:0.5vw;width:3.6vw;margin:0 0 0 0.5vw}

.noti_view .cont {width:94%;margin:0 auto;padding:3vw 0 0 0;border:1px solid #ccc;border-width:1px 0 1px 0;color:#343434}

.noti_view .down img {float:left;width:4vw !important;margin-right:2vw;padding-top:1.5vw}
.noti_view .down a span {font-size:3vw;color:#b4b4b4}

.noti_view .cont ul.sns {position:absolute;right:2vw;margin-bottom:5vw}
.noti_view .cont ul.sns li {float:left;height:8vw;margin-right:2vw}
.noti_view .cont ul.sns a img {width:7.5vw !important}

.noti_view .cont p {padding:6vw 0;white-space:pre-line;word-break:break-all}
.noti_view .cont img {width:100%}

/* 커뮤니티 글쓰기 */

.txt-type {padding:4vw 0 2vw;border-bottom:1px solid #e0e0e0;font-size:4vw;font-weight:700}
.txt-type-01 {padding:6vw 0 3vw;font-size:4.16vw;font-weight:700}
.txt-type-01 span {color:#66aa0e}
.txt-type-02 {padding:6vw 0;border-bottom:1px dotted #959595;text-align:center}

.form_table {margin:3vw 0 1vw}

.form_table table {width:100%;padding:0 0 3vw;border:0}
.form_table caption {padding:1vw 0;text-align:left}
.form_table th,
.form_table td {padding:2vw 0;border:0;font-weight:400}
.form_table th {width:26%}
.form_table td {width:74%;color:#424242}

.form_table tbody th {color:#66aa0e;text-align:left}

.form_table tbody th span {color:#ed1919}

.form_table .item {position:relative}
.form_table .item .i_label {top:6px;left:10px}
.form_table .item .i_dsc {margin:2px 0;font-size:2.5vw;color:#908f8f}
.form_table .item_wrap .i_text {width:100%}

.form_table .item .i_check,
.form_table .item .i_radio {width:3.6vw;height:3.6vw;margin:0.5vw;padding:0;vertical-align:middle}
.form_table .item .i_text {position:relative;height:7vw;margin:0.5vw;border:1px solid #808080;background:transparent;line-height:7vw;padding-left: 1vw;}

.form_table .item select {width:32%;height:7vw;margin:0.5vw;padding:0 1vw;border:1px solid #808080;line-height:7vw;background:url(../img/common/select_arrow.png) no-repeat 93% 50%;background-size:2.29vw 1.38vw;-webkit-appearance:none;-moz-appearance:none;appearance:none}
.form_table .item label {display:inline-block;margin:0 6vw 0 0.6vw;cursor:pointer}

#sample2_address2::-webkit-input-placeholder {font-size:3.125vw;color:#b1b1b1;text-align:left}

.label.t_radio {position: relative; padding-left: 6.34vw; font-size: 3.17vw; line-height: 5.36vw; color: #424242;}
.label.t_radio:before {content:"";position:absolute;top:0;left:0;width:4.8vw;height:4.8vw;background:transparent url(../img/common/ico_radio.png) no-repeat center center;background-size:4.6vw auto}
.radio:checked + .label.t_radio:before {background-image: url(../img/common/ico_radio_press.png)}
.radio {position:absolute;z-index:-1;left:-999px;width:0.24vw;height:0.24vw;-webkit-appearance:none;appearance:none}

.none {margin:0 0 5vw;padding:8vw 0;border:1px solid #808080;background-color:#f8f8f8;text-align:center}

/* 버튼 */

.form_table .item .btn {display:inline-block;width:12vw;height:7vw;margin:0.5vw;border:1px solid #66aa0e;background:#66aa0e;font-size:3vw;color:#fff;text-align:center;line-height:7vw}
.form_table .btn_cen {padding:2vw 0;text-align:center}

/* 아이디 */

.form_table .i_wrap {width:100%;text-align:center}
.form_table .i_wrap:after {display:block;clear:both;content:""}
.form_table .i_wrap span {float:left;width:72%;line-height:0}
.form_table .i_wrap span + span {float:left;width:26%;margin-left:2%}

/* 이메일 */

.form_table .item_wrap2 {width:100%;text-align:center}
.form_table .item_wrap2:after {display:block;clear:both;content:""}
.form_table .item_wrap2 span {float:left;width:39%}
.form_table .item_wrap2 span + span {float:left;width:12%}
.form_table .item_wrap2 span + span + span {float:left;width:49%}
.form_table .item_wrap2 span#whelk {position:relative;top:1.1vw;left:0.5vw}

/* 자주묻는질문 */

.cont_question select {width:23.81vw;height:7vw;margin:4vw 0;padding:0 1vw;border:1px solid #d9d9d9;background:url(../img/common/select_arrow.png) no-repeat 93% 50%;background-size:2.29vw 1.38vw;line-height:7vw;-webkit-appearance:none;-moz-appearance:none;appearance:none}

.faq_list {margin:0;border-top:1px solid #ccc;border-bottom:1px solid #ccc}

.faq_list dt {border-top:1px solid #ccc}
.faq_list dt:first-child {border-top:0}
.faq_list dl dt a {display:block;position:relative;padding:4vw 2vw}
.faq_list dl dt a:after {content:"";position:absolute;top:50%;right:5vw;;width:2.29vw;height:1.38vw;margin-top:-0.69vw;background:url(../img/common/faq_arrow.png) no-repeat 0 0;background-size:2.29vw 1.38vw}
.faq_list dl dt a.on:after {background:url(../img/common/faq_arrow_on.png) no-repeat 0 0;background-size:2.29vw 1.38vw}

.faq_list dl dt a .tit {display:block;font-size:3.5vw}
.faq_list dl dd {display:none;position:relative;padding:3vw 3%;border-top:1px solid #ccc;background-color:#f8f8f8;font-size:3vw;font-weight:500;color:#424242}
.faq_list dl dd img {width:100% !important;margin:3vw 0}

/* 내문의확인 */

tr.fna_question {cursor:pointer}
tr.fna_question td:first-child {position:relative;text-align:left}
tr.fna_question td:first-child:after {content:"";position:absolute;top:50%;right:3vw;;width:2.29vw;height:1.38vw;margin-top:-0.69vw;background:url(../img/common/faq_arrow.png) no-repeat 0 0;background-size:2.29vw 1.38vw}
tr.fna_question.active td:first-child:after {background:url(../img/common/faq_arrow_over.png) no-repeat 0 0;background-size:2.29vw 1.38vw}

tr.fna_question.active td:first-child {background-color:#424242;color:#fff}
tr.fna_question.active td + td {background-color:#424242;color:#fff}
tr.fna_question.active td + td em {color:#fff !important}

tr.fna_answer td {font-weight:500 !important}

.fna_container tbody tr.fna_answer td {padding:0}
.fna_container tbody tr.fna_answer td div {opacity:1;padding:5vw 4vw 0;background:#f8f8f8;text-align:left;transition:opacity 0.6s 0.1s;-webkit-transition:opacity 0.6s 0.1s}
.fna_container tbody tr.fna_answer td div + div {padding-top:0}
.fna_container tbody tr.fna_answer td div strong {display:block;margin-bottom:5vw;padding:0 0 0.5vw;font-size:3.3vw;font-weight:500}
.fna_container tbody tr.fna_answer td div span.cont {position:relative;top:-30px;font-size:3vw;font-weight:400;white-space:pre-line}
.fna_container tbody tr.fna_answer.fna_hidden td {border-bottom:0}

/* 문의하기 */

.form_table.ver2 th {width:14%}
.form_table.ver2 td {width:86%}

.form_table.ver2 tbody th {padding:2.5vw 0;border:1px solid #b5b5b5;border-width:0 0 1px}
.form_table.ver2 tbody td {padding:2.5vw 0;border:1px solid #b5b5b5;border-width:0 0 1px}

.form_table.ver2 tbody tr:nth-child(3n) th,
.form_table.ver2 tbody tr:nth-child(4n) th {padding:2vw 0 0;border:0}
.form_table.ver2 tbody tr:nth-child(3n) td,
.form_table.ver2 tbody tr:nth-child(4n) td {border:0}

/* 내정보 (소비자) */

.tbl_info {width:100%;margin:4vw 0 0}
